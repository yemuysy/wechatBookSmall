.avatar {
  height: 148rpx;
  width: 148rpx;
  border-radius: 50%;
}

.top {
  width: 100%;
  height: 250rpx;
  display: flex;
  flex-direction: column;
  background-color: #ab956d;
  align-items: center;
  justify-content: center;
}

.nickname {
  margin-bottom: 10rpx;
  text-align: center;
}

.item {
  align-items: center;
  height: 70rpx;
  margin: 20rpx;
  display: flex;
  border-radius: 20rpx;
  background-color: #f4f4f4;
}
